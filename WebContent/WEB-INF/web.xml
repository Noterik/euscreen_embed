<?xml version="1.0" encoding="UTF-8"?>
<web-app id="euscreen_embed" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee">
	<display-name>EUScreen Embed service</display-name>
	<servlet>
		<servlet-name>EuscreenEmbedServlet</servlet-name>
		<servlet-class>org.springfield.euscreen.servlet.EuscreenEmbedServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EuscreenLibServlet</servlet-name>
		<servlet-class>org.springfield.euscreen.servlet.EuscreenLibServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EuscreenEmbedServlet</servlet-name>
		<url-pattern>/euscreen_embed</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EuscreenLibServlet</servlet-name>
		<url-pattern>/euscreenembedlib.js</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>